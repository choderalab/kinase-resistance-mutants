########## Virtual Screening Workflow Input File ###############
# Run as: $SCHRODINGER/vsw <inputfile>

[SET:ORIGINAL_LIGANDS]
    VARCLASS   Structures
    FILES   ligprep_fda-out.maegz,

[SET:GRID_1]
    VARCLASS   Grid
    FILE   grid-axitinib.zip

[SET:GRID_2]
    VARCLASS   Grid
    FILE   grid-dasatinib.zip

[SET:GRID_3]
    VARCLASS   Grid
    FILE   grid-bosutinib.zip

[SET:GRID_4]
    VARCLASS   Grid
    FILE   grid-erlotinib.zip

[SET:GRID_5]
    VARCLASS   Grid
    FILE   grid-gefitinib.zip

[SET:GRID_6]
    VARCLASS   Grid
    FILE   grid-imatinib.zip

[SET:GRID_7]
    VARCLASS   Grid
    FILE   grid-nilotinib.zip

[SET:GRID_8]
    VARCLASS   Grid
    FILE   grid-ponatinib.zip

[STAGE:PRE_DOCK_XP_1]
    STAGECLASS   gencodes.RecombineStage
    INPUTS   ORIGINAL_LIGANDS,
    OUTPUTS   RECOMBINE_OUT_1,
    NUMOUT   njobs
    OUTFORMAT   maegz
    MIN_SUBJOB_STS   20
    MAX_SUBJOB_STS   200
    GENCODES   YES
    OUTCOMPOUNDFIELD   s_vsw_compound_code
    OUTVARIANTFIELD   s_vsw_variant
    UNIQUEFIELD   NONE

[STAGE:DOCK_XP_1]
    STAGECLASS   glide.DockingStage
    INPUTS   RECOMBINE_OUT_1, GRID_1
    OUTPUTS   XP_OUT_1,
    RECOMBINE   NO
    PRECISION   XP
    UNIQUEFIELD   s_vsw_compound_code
    PERCENT_TO_KEEP   100.0
    DOCKING_METHOD   confgen
    POSES_PER_LIG   3
    WRITE_XP_DESC   YES
    BEST_BY_TITLE   NO
    LIG_VSCALE   0.8
    LIG_CCUT   0.15
    MAXATOMS   300
    MAXROTBONDS   50
    WRITE_RES_INTERACTION   YES
    RADIUS_RES_INTERACTION   12.0
    AMIDE_MODE   penal
    POSE_OUTTYPE   PV
    POSTDOCK   YES
    POSTDOCKSTRAIN   NO
    COMPRESS_POSES   YES
    EPIK_PENALTIES   YES
    FORCEPLANAR   NO

[STAGE:MMGBSA_1]
    STAGECLASS   prime.MMGBSAStage
    INPUTS   XP_OUT_1,
    OUTPUTS   MMGBSA_1,

